{
  "name": "ceres-framework",
  "version": "0.13.9",
  "description": "Node.js Framework for React Applications",
  "main": "src/index.js",
  "bin": {
    "ceres": "./cli/index.js"
  },
  "scripts": {
    "test": "npm run lint && ./node_modules/.bin/istanbul cover --include-all-sources -x 'cli/**' -x 'spec/**' -x 'config/**' ./node_modules/.bin/jasmine",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "./node_modules/.bin/eslint cli/ src/ config/",
    "lint:fix": "./node_modules/.bin/eslint --fix cli/ src/ config/",
    "watch:test": "./node_modules/.bin/chokidar --initial --verbose -p 'spec/**/*.js' -p 'src/**/*.js' -c 'npm test'"
  },
  "author": "Isaac Suttell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ship-components/ceres-framework.git"
  },
  "engines": {
    "node": ">=4.5.0 <9"
  },
  "dependencies": {
    "async": "0.9.2",
    "base32": "0.0.6",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "commander": "^2.11.0",
    "compression": "^1.7.0",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "hashids": "^1.1.1",
    "knex": "^0.12.9",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "redis": "^2.7.1",
    "response-time": "^2.3.2",
    "winston": "^2.3.1"
  },
  "optionalDependencies": {
    "express-socket.io-session": "^1.3.1",
    "mongodb": "^2.2.11",
    "rethinkdb": ">=2.3.0",
    "socket.io": "^1.5.1",
    "pg-live-select": ">=1.0.3",
    "pg": ">=7.3.0",
    "bookshelf": "^0.10.3",
    "react": ">=15",
    "react-dom": ">=15",
    "sticky-session": "^1.1.2",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.10.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.3.0",
    "eslint-config-ship-components-base": "^1.1.0",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.7.0"
  }
}
